<?xml version="1.0"?>
<block>
  <name>Burst to FN time</name>
  <key>gsm_burst_to_fn_time</key>
  <import>import grgsm</import>
  <make>grgsm.burst_to_fn_time()</make>

  <sink>
    <name>bursts_in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <source>
    <name>fn_time_out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
